layout {

    focus-ring {
        active-color   "#9bcaff"
        inactive-color "#121316"
        urgent-color   "#ffb4ab"
    }

    border {
        active-color   "#9bcaff"
        inactive-color "#121316"
        urgent-color   "#ffb4ab"
    }

    shadow {
        color "#00000070"
    }

    tab-indicator {
        active-color   "#9bcaff"
        inactive-color "#00497a"
        urgent-color   "#ffb4ab"
    }

    insert-hint {
        color "#9bcaff80"
    }
}

recent-windows {
    highlight {
        active-color "#9bcaff"
        urgent-color "#ffb4ab"
    }
}
